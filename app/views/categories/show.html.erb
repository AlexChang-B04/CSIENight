<div class="header">
  <h1>Problem Set</h1>
  <h2><%= @category.name %></h2>
  <% flash.each do |name,msg|%>
    <h2><%= msg %></h2>
  <% end %>
</div>
<div class="content">
  <ul class="Set">
    <% @questions.each do |question| %>
      <%= render 'question', {:question => question} %>
    <% end %>
  </ul>
</div>
<p class="back-link"><%= link_to "Back to Problem Set Menu",categories_path %><p>

